module std

enum Bool {
  True
  False
} {
  operator fn equals(o: Bool) = self == o
  operator fn and(o: bool) = self == false || o == false ? false : true
  operator fn or(o: bool) = self == true || o == true ? true : false
  operator fn not(): bool = self == true ? false : true
  fn toStr() = self ? "true" : "false"
}
