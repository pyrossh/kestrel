{
  "name": "kestrel",
  "displayName": "kestrel",
  "description": "Textmate grammar for kestrel",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kestrel",
        "aliases": [
          "kestrel"
        ],
        "extensions": [
          ".ks"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kestrel",
        "scopeName": "source.ks",
        "path": "./syntaxes/kestrel.tmLanguage.json"
      }
    ]
  }
}